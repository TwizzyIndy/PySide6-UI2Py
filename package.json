{
  "name": "pyside6-ui2py",
  "displayName": "PySide6 UI2Py",
  "description": "The extension for compiling PySide6 UI files",
  "version": "0.0.1",
  "publisher": "AungKhantM",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/TwizzyIndy/PySide6-UI2Py.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
	"extensionDependencies": [
		"ms-python.python"
	],
	"dependencies": {
		"@vscode/python-extension": "^1.0.5",
		"@types/vscode": "^1.95.0"
	},
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.runCommand",
        "title": "Compile UI2Py"
      },
      {
        "command": "extension.openInQtDesigner",
        "title": "Open in Qt Designer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.runCommand",
          "when": "resourceExtname == .ui",
          "group": "navigation"
        },
        {
          "command": "extension.openInQtDesigner",
          "when": "resourceExtname == .ui",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
